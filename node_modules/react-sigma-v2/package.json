{
  "name": "react-sigma-v2",
  "version": "1.0.3",
  "description": "React Sigma components",
  "author": "Benoit Simard",
  "license": "MIT",
  "homepage": "https://sim51.github.io/react-sigma-v2",
  "bugs": "https://github.com/sim51/react-sigma-v2/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/sim51/react-sigma-v2.git"
  },
  "keywords": [
    "sigma",
    "react",
    "graph",
    "graphology"
  ],
  "main": "./lib/umd/react-sigma-v2.min.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/esm/",
  "exports": {
    ".": "./lib/esm/index.js"
  },
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "clean": "rimraf lib/",
    "lint": "esw . --ext .ts",
    "lint:examples": "esw ./examples --ext .ts --ext .tsx",
    "lint:all": "npm run lint && npm run lint:examples",
    "compile": "npm run tsc",
    "compile:examples": "npm run tsc:examples",
    "compile:all": "npm run tsc:all",
    "build": "npm run clean && npm run lint && npm run compile && webpack --progress",
    "examples": "webpack serve --config ./examples/webpack.config.js --progress",
    "examples:build": "webpack build --config ./examples/webpack.config.js --progress",
    "site": "rimraf docs/ && typedoc && npm run examples:build",
    "tsc": "tsc -p ./tsconfig.base.json",
    "tsc:examples": "tsc -p ./examples/tsconfig.json  --noEmit",
    "tsc:all": "npm run tsc && npm run tsc:examples",
    "test:e2e": "mocha --require ts-node/register ./test/e2e/**/*.spec.ts --exit",
    "test:e2e:generate": "node -r ts-node/register ./test/e2e/generate.ts",
    "test": "npm run tsc:all && npm run test:e2e"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@types/chroma-js": "^2.1.1",
    "@types/faker": "^5.5.3",
    "@types/lodash": "4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/seedrandom": "^3.0.1",
    "@types/webpack-dev-server": "^3.11.5",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chroma-js": "^2.1.1",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-watch": "^7.0.0",
    "faker": "^5.5.3",
    "graphology": "^0.21.0",
    "graphology-generators": "^0.11.0",
    "graphology-layout": "^0.4.0",
    "graphology-layout-forceatlas2": "^0.6.1",
    "graphology-types": "^0.19.5",
    "html-webpack-plugin": "^5.3.1",
    "jsdom": "16.5.3",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^1.5.0",
    "mocha": "^8.3.2",
    "pixelmatch": "^5.2.1",
    "puppeteer": "^10.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-icons": "^4.3.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "save-dev": "*",
    "seedrandom": "^3.0.5",
    "sigma": "latest",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.1.0",
    "ts-node": "^9.1.1",
    "typedoc": "0.21.2",
    "typescript": "^4.3.5",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "graphology": ">=0.20.0",
    "graphology-layout-forceatlas2": ">=0.6.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "sigma": "^2.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
